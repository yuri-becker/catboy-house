{{ auth_lldap_domain }} {
	import common-headers
    import logging {{ auth_lldap_domain }}
    import forward-auth {{ docker_gateway }}:{{ auth_authelia_port }}
	reverse_proxy {{ docker_gateway }}:{{ auth_lldap_port }}
}

{{ auth_authelia_domain }} {
	import common-headers
    import logging {{ auth_authelia_domain }}
	reverse_proxy {{ docker_gateway }}:{{ auth_authelia_port }}
}