syntax on
set autoindent
set expandtab
set softtabstop =2
set shiftwidth =2
set shiftround

set backspace =indent,eol,start
set hidden
set laststatus =2
set display =lastline

set showmode
set showcmd

set incsearch
set hlsearch
set number

set ttyfast
set lazyredraw

set splitbelow
set splitright

set cursorline
set wrapscan
set report =0
set synmaxcol =200